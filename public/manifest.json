{
    "name": "OpenShift Project Alias",
    "description": "This extension allows you to create aliases for OpenShift projects.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/modify.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}